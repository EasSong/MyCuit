<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.cuit.model.Question" table="question_tab">
        <id name="q_Id">
            <column name="q_id"/>
        </id>
        <property name="q_Name">
            <column name="q_name"/>
        </property>
        <property name="q_Type">
            <column name="q_type"/>
        </property>
        <property name="q_Options">
            <column name="q_options"/>
        </property>
    </class>
</hibernate-mapping>